<!DOCTYPE html>
<html>
	<head>
		<title>Twitter</title>
		<%= csrf_meta_tags %>

		<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
		<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

	</head>

	<body>

<!-- container to keep it neat  -->
		<div class="container text-center">
			<!-- render Nav Bar  -->
			<%= render 'layouts/nav'%>
			<%= image_tag "burgerking.svg.png", class: "burgerking"%>
			<!--Bird image  -->

			<%= image_tag "burgerking.svg.png", class: "img-fluid text-center logo"%>


			<p class="notice"><%= notice %></p>
			<p class="alert"><%= alert %></p>
			<div class="jumbotron">

				<%= yield %>
			</div>
		</div>
	</body>
</html>
